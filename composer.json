{
    "name": "tustin/psn-php",
    "license": "MIT",
    "description": "PHP wrapper for the PlayStation API.",
    "homepage": "https://github.com/Tustin/psn-php",
    "keywords": ["playstation", "api", "php"],
    "authors": [
        {
            "name": "Tustin",
            "email": "tustin@tusticles.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tustin\\PlayStation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Tustin\\PlayStation\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "nesbot/carbon": "^2.29",
        "myclabs/php-enum": "^1.7",
        "wapmorgan/mp3info": "^0.0.5",
        "tustin/haste": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "phan/phan": "^2.4"
    },
    "scripts": {
        "dev" : "@php ./tests/test.php",
        "test" : "phpunit",
        "analyze": "phan"
    }
}
